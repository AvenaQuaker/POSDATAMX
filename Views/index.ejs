		<div class="absolute inset-0 bg-black/60 flex items-center justify-center text-white text-xl font-bold opacity-0 group-hover:opacity-100 transition duration-300">
		<%= translations.galeria.cards[5] %>
		</div>
	</div>
</div>
</section>
<!--====================================== GALLERY END ========================================================-->
<!--======================================= TEAM START ========================================================-->
<section id="equipo" class="py-16 bg-gray-200">
	<div class="box-content max-w-6xl mx-auto px-6 grid grid-cols-1 md:grid-cols-[1fr_2fr] gap-12">
		<div data-aos="fade-right">
		<h2 class="text-3xl font-bold text-gray-800 mb-2"><%= translations.equipo.titulo %></h2>
		<div class="w-28 h-2 bg-gray-300 mb-6"></div>
		<p class="text-gray-600 mb-6">
			<%= translations.equipo.descripcion %>
		</p>
		<a href="#"
			class= "hover:scale-105 transition bg-red-600 hover:bg-red-700 text-white font-semibold px-6 py-3 rounded">
			<%= translations.equipo.leer_mas %>
		</a>
		</div>
		<div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-2" data-aos="fade-up">
		<% translations.equipo.miembros.forEach(function(m) { %>
			<div class="text-center">
				<div style="background-image: url('<%= m.img %>');" class="w-full h-64 bg-gray-300 flex items-center justify-center text-gray-500 mb-4 bg-cover bg-center hover:scale-105 transition">
				
				</div>
				<h3 class="font-bold text-gray-800 <%= m.nombre_mayusculas ? 'uppercase' : 'capitalize' %>"><%= m.nombre %></h3>
				<p class="text-red-600 font-semibold text-sm mb-3"><%= m.rol %></p>
			</div>
		<% }) %>
		</div>
	</div>
</section>
<!--======================================= TEAM END ========================================================-->
<!--======================================= NEWS START ========================================================-->
<section class="py-16 bg-[url('../images/WhiteBG1.jpg')] bg-fixed bg-cover">
