<!--======================================= TESTIMONIAL START ========================================================-->
<section class="py-16 bg-[#141414ff] text-white">
	<div class="max-w-6xl mx-auto px-6 text-center box-content">

		<h2 class="text-3xl font-bold" data-aos="fade-down"><%= translations.testimonios.titulo %></h2>
		<div class="w-28 h-2 bg-[#d9171f] mx-auto my-4" data-aos="fade-down"></div>
	
		<div class="grid grid-cols-1 md:grid-cols-3 gap-8 mt-12">
		<% translations.testimonios.personas.forEach(function(t) { %>
	
			<div class="flex flex-col items-center" data-aos="zoom-in-up">
				<div style="background-image: url('<%= t.img %>')" class="bg-cover bg-center w-24 h-24 bg-gray-300 rounded-full flex items-center justify-center text-gray-700 mb-6">
			
				</div>
				<p class="text-gray-300 mb-4">
				<%= t.mensaje %>
				</p>
				<p class="font-bold">
				<%= t.nombre %> <span class="text-red-500"><%= t.rol %></span>
				</p>
			</div>
		<% }) %>
		</div>
	</div>
</section>
<!--======================================= TESTIMONIAL END ========================================================-->
<!--======================================= CONTACT US START =======================================================-->
<section id="contacto" class="py-16 bg-white">
	<div class="max-w-6xl mx-auto px-6 text-center box-content">
		
		<h2 class="text-3xl font-bold text-gray-800"  data-aos="fade-down"><%= translations.contactanos.titulo %></h2>
		<div class="w-24 h-2 bg-gray-300 mx-auto my-4"  data-aos="fade-down"></div>
		<p class="text-gray-600 mb-8"  data-aos="fade-down">
			<%= translations.contactanos.descripcion %>
		</p>
		
		<div class="flex flex-row gap-6"  data-aos="zoom-in">
			<form class="w-full space-y-6 text-left">
			<div class="flex flex-row w-full gap-2">
				<div class="w-full">
					<label for="name" class="block text-sm font-medium text-gray-700 mb-1"><%= translations.contactanos.form.nombre %></label>
					<input type="text" id="name" class="w-full border border-gray-300 rounded px-3 py-2 focus:outline-none focus:ring-2 focus:ring-red-600">
				</div>
				<div class="w-full">
					<label for="empresa" class="block text-sm font-medium text-gray-700 mb-1"><%= translations.contactanos.form.empresa %></label>
					<input type="text" id="emp" class="w-full border border-gray-300 rounded px-3 py-2 focus:outline-none focus:ring-2 focus:ring-red-600">
				</div>
			</div>
			<div class="flex flex-row w-full gap-2">
				<div class="w-full">
					<label for="tel" class="block text-sm font-medium text-gray-700 mb-1"><%= translations.contactanos.form.tel %></label>
					<input type="tel" id="tel" class="w-full border border-gray-300 rounded px-3 py-2 focus:outline-none focus:ring-2 focus:ring-red-600">
				</div>
				<div class="w-full">
					<label for="email" class="block text-sm font-medium text-gray-700 mb-1"><%= translations.contactanos.form.email %></label>
					<input type="email" id="email" class="w-full border border-gray-300 rounded px-3 py-2 focus:outline-none focus:ring-2 focus:ring-red-600">
				</div>
			</div>
			<div>
				<label for="message" class="block text-sm font-medium text-gray-700 mb-1"><%= translations.contactanos.form.mensaje %></label>
				<textarea id="message" rows="4" class="w-full border border-gray-300 rounded px-3 py-2 focus:outline-none focus:ring-2 focus:ring-red-600"></textarea>
			</div>
			
			<button type="submit" class="bg-red-600 hover:bg-red-700 text-white font-semibold px-6 py-3 rounded">
				<%= translations.contactanos.form.enviar %>
			</button>
			</form>
			<div style="background-image: url('../images/Contact.jpg');"class="bg-cover bg-center w-full  bg-gray-600 rounded"></div>
        </div>
	</div>
</section>
<!--======================================= CONTACT US END =======================================================-->
<!--======================================= FOOTER START =======================================================-->
