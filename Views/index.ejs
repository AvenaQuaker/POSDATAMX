<!DOCTYPE html>
<html class="scroll-smooth" lang="<%= lang %>">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>POSDATAMX</title>
        <link rel="stylesheet" href="css/mainTW.css">
		<link rel="stylesheet" href="aos/aos.css" />
		<script src="aos/aos.js"></script>
</head>
<body class="bg-sky-50 w-full h-full font-Montserrat">
<!--================================= NAVIGATION START ==========================================-->
<header>
	<nav id="mainNav" 
		class="bg-[#2b2b2b] fixed top-0 left-0 w-full py-3 px-4 shadow-md transition-all duration-500 -translate-y-full opacity-0 z-30">
		<div class="flex justify-between items-center mx-6 lg:mx-40">
		<!-- Logo -->
		<img src="images/LogoBlanco.png" alt="150x50" class="h-10">

		<!-- Opciones en pantallas grandes -->
		<div class="hidden md:flex space-x-6">
		<a id="navInicio" href="#inicio" class="text-gray-200 hover:text-red-800 transition-colors font-semibold text-[1rem]"><%= translations.nav.inicio %></a>
		<a id="navAcerca" href="#acerca" class="text-gray-200 hover:text-red-800 transition-colors font-semibold text-[1rem]"><%= translations.nav.acerca %></a>
		<a id="navServicios" href="#servicios" class="text-gray-200 hover:text-red-800 transition-colors font-semibold text-[1rem]"><%= translations.nav.servicios %></a>
		<a id="navEquipo" href="#equipo" class="text-gray-200 hover:text-red-800 transition-colors font-semibold text-[1rem]"><%= translations.nav.equipo %></a>
		<a id="navContacto" href="#contacto" class="text-gray-200 hover:text-red-800 transition-colors font-semibold text-[1rem]"><%= translations.nav.contacto %></a>
		</div>

		<!-- BotÃ³n hamburguesa (visible en mÃ³viles) -->
		<button id="menuBtn" class="md:hidden text-gray-700 focus:outline-none">
			<!-- Ã­cono hamburguesa -->
			<svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8" fill="none" viewBox="0 0 24 24" stroke="currentColor">
			<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16" />
			</svg>
		</button>
		</div>
	</nav>

	<!-- MenÃº lateral (off-canvas) -->
	<div id="sideMenu" 
		class="fixed top-0 right-0 w-64 h-full bg-stone-50 shadow-lg transform translate-x-full transition-transform duration-300 z-50">
		<div class="flex justify-between items-center p-4 border-b">
		<span class="font-bold text-gray-800">MenÃº</span>
		<!-- BotÃ³n cerrar -->
		<button id="closeMenu" class="text-gray-700">
			<svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
			<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12" />
			</svg>
		</button>
		</div>
		<div class="flex flex-col p-6 space-y-4">
		<a id="menuInicio" href="#inicio" class="text-gray-700 hover:underline font-semibold"><%= translations.nav.inicio %></a>
		<a id="menuAcerca" href="#acerca" class="text-gray-700 hover:underline font-semibold"><%= translations.nav.acerca %></a>
		<a id="menuServicios" href="#servicios" class="text-gray-700 hover:underline font-semibold"><%= translations.nav.servicios %></a>
		<a id="menuEquipo" href="#equipo" class="text-gray-700 hover:underline font-semibold"><%= translations.nav.equipo %></a>
		<a id="menuContacto" href="#contacto" class="text-gray-700 hover:underline font-semibold"><%= translations.nav.contacto %></a>
		</div>
	</div>
</header>
<div id="overlay" 
class="fixed inset-0 bg-black/50 hidden z-40"></div>

<!--================================= NAVIGATION END ==========================================-->
<section id="inicio" class="bg-[url(https://www.hermanmiller.com/content/dam/hmicom/page_assets/research/case_studies/concept_a/mh_cs_concept_a.jpg)] bg-fixed bg-cover h-[60rem] flex justify-center items-center">
	<div class="bg-black/70 h-full w-[70%] lg:w-[30%] flex flex-col justify-center items-center text-center gap-4">
		<img src="images/LogoBlanco.png" alt="128x128x1" class="h-48" data-aos="zoom-in">
		<a href="#" class="bg-red-800 text-white px-6 py-3 rounded-full font-sams font-semibold text-lg mx-40 mt-6 inline-block hover:bg-red-700"><%= translations.inicio.contactanos %></a>
	</div>
</section>
<!--================================= ABOUT US START ==========================================-->
<section id="acerca" class="py-18 bg-gray-200 ">
	<div class="max-w-6xl mx-auto px-6 grid grid-cols-1 md:grid-cols-2 gap-12 items-center justify-center box-content" data-aos="fade-down">
		<div class="flex flex-col items-center justify-center" >
		<h2 id="aboutTitulo" class="text-2xl font-bold text-gray-800 mb-4"><%= translations.acerca.titulo %></h2>
		<div class="w-24 h-2 bg-gray-300 mb-6"></div>

		<p id="aboutDescripcion" class="text-gray-600 mb-6 text-[14px]">
			<%= translations.acerca.descripcion %>
		</p>

	<div id="aboutCaracteristicas" class="grid grid-cols-2 gap-4 mb-6 w-full"  >
		<% translations.acerca.caracteristicas.forEach(function(c) { %>
			<div class="flex items-center space-x-3">
			<div class="w-10 h-10 bg-gray-300 flex items-center justify-center text-sm font-bold text-gray-500">
				*
			</div>
			<span class="font-semibold text-gray-700"><%= c %></span>
			</div>
		<% }) %>
	</div>

		<a href="#" id="aboutLeer" class="inline-block bg-red-600 hover:bg-red-700 text-white font-semibold px-4 py-2 rounded">
			<%= translations.acerca.leer_mas %>
		</a>
		</div>
		<img src="images/Imagen1.jpg" alt="128x128x1" class="h-[25rem] w-full rounded bg-cover bg-center hover:scale-105 transition">
	</div>
</section>
<!--================================= ABOUT US END ==========================================-->
<!--================================= OUR SERVICES ==========================================-->
<section id="servicios" class="py-16 bg-[url('../images/WhiteBG1.jpg')] bg-fixed bg-cover">
<div class="max-w-6xl mx-auto px-6 text-center box-content " data-aos="fade-down">

<h2 id="" class="text-3xl font-bold text-gray-800"><%= translations.servicios.titulo %></h2>
<div class="w-24 h-2 bg-[#d9171f] mx-auto my-4"></div>

<div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-8 mt-12">
	<% translations.servicios.cards.forEach(function(c) { %>
			<!-- Card -->
			<div class="rounded-xl border bg-white border-gray-200 p-8 hover:shadow-lg transition" data-aos="zoom-in">
			<img class="w-12 h-12 my-2 mx-auto" alt="seccionImg" src="<%= c.img %>">
			<h3 class="text-lg font-bold text-gray-800 mb-2"><%= c.titulo %></h3>
			<p class="text-gray-600 mb-4">
				<%= c.descripcion %>
			</p>
			<a href="/Servicios/<%= lang %>/<%= c.sec %>" class="text-red-600 font-semibold hover:underline text-sm tracking-wide"><%= c.leer_mas %></a>
			</div>
	<% }) %>
</div>
</div>
</section>
<!--================================= OUR SERVICES END ==========================================-->
<!--================================= QUOTE START ==========================================-->
<section class="py-20 bg-[#141414ff]">
	<div class="max-w-6xl mx-auto box-content px-6 grid grid-cols-1 md:grid-cols-[4fr_6fr] gap-12 items-center">
		<img src="images/Imagen2.jpg" alt="128x128x1" class="h-96 w-full rounded hover:scale-105 transition" data-aos="fade-right">
		<div data-aos="fade-left">
		<h2 class="text-2xl font-bold text-stone-100 mb-2"><%= translations.contacto.titulo %></h2>
		<div class="w-24 h-2 bg-gray-300 mb-6"></div>
		
		<p class="text-gray-400 mb-4">
			<%= translations.contacto.descripcion1 %>
		</p>
		<p class="text-gray-400 mb-6">
			<%= translations.contacto.descripcion2 %>
		</p>
		<div class="rounded border border-gray-400 p-6 flex flex-col md:flex-row md:items-center justify-between gap-4">
			<div>
			<h3 class="font-semibold text-stone-100"><%= translations.contacto.sugerencia %></p>
			</div>
			<a href="#"
			class="bg-red-600 hover:bg-red-700 text-white font-semibold px-6 py-3">
			<%= translations.contacto.cotizar %>
			</a>
		</div>
		</div>
	</div>
</section>
<!--================================= QUOTE END ==========================================-->
<!--================================= GALLERY START ==========================================-->
